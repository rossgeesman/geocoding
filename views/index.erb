<div class="container-fluid">
  <div class="row">
    <div class="col-md-offset-4 col-md-4">
      <h1 class="bg-primary text-center p25">
        Geocoder
      </h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-offset-4 col-md-4">
      <form id="addressSearch">
        <div class="form">
          <div class="col">
            <input type="text" name="lat" class="form-control" placeholder="Latitude">
          </div>
          <div class="col">
            <input type="text" name="lng" class="form-control" placeholder="Longitude">
          </div>
        </div>
        <button type="submit" class="btn btn-primary">Find Address</button>
      </form>
      <h4> Location List </h4>
      <ul class="list-group" id="locationsList">
      </ul>
    </div>
  </div>
</div>
